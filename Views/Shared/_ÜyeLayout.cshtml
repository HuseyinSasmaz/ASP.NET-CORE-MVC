
@{
    Layout = null;
}


<!DOCTYPE html>
<html lang="en">

@await Html.PartialAsync("/Views/ÜyelerLayoutPartial/_HeaderPartial.cshtml")

<body class="g-sidenav-show  bg-gray-100">
   
    @await Html.PartialAsync("/Views/ÜyelerLayoutPartial/_NavbarPartial.cshtml")

    <main class="main-content position-relative max-height-vh-100 h-100 mt-1 border-radius-lg ">
        <!-- Navbar -->
        @await Html.PartialAsync("/Views/ÜyelerLayoutPartial/_NavbarPartial1.cshtml")
        
        <!-- End Navbar -->
        <div class="container-fluid py-4">
            
            @RenderBody()

            @await Html.PartialAsync("/Views/ÜyelerLayoutPartial/_FooterPartial.cshtml")
        </div>
    </main>

    @await Html.PartialAsync("/Views/ÜyelerLayoutPartial/_RightbarPartial.cshtml")

    <!--   Core JS Files   -->
    <script src="/ÜyeDashboard/assets/js/core/popper.min.js"></script>
    <script src="/ÜyeDashboard/assets/js/core/bootstrap.min.js"></script>
    <script src="/ÜyeDashboard/assets/js/plugins/perfect-scrollbar.min.js"></script>
    <script src="/ÜyeDashboard/assets/js/plugins/smooth-scrollbar.min.js"></script>
    <script>
        var win = navigator.platform.indexOf('Win') > -1;
        if (win && document.querySelector('#sidenav-scrollbar')) {
            var options = {
                damping: '0.5'
            }
            Scrollbar.init(document.querySelector('#sidenav-scrollbar'), options);
        }
    </script>
    <!-- Github buttons -->
    <script async defer src="https://buttons.github.io/buttons.js"></script>
    <!-- Control Center for Soft Dashboard: parallax effects, scripts for the example pages etc -->
    <script src="/ÜyeDashboard/assets/js/soft-ui-dashboard.min.js?v=1.0.3"></script>
</body>

</html>

